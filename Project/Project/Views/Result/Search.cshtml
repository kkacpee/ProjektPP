@model IEnumerable<Photo>
@{
    ViewData["Title"] = "Search";
}

<h1>Search</h1>

<div class="card-columns">
    @foreach (var photo in Model)
    {
        <div class="card">
            <a asp-controller="Result" asp-action="Result" asp-route-id="@photo.ID">
                <img src="@photo.Url" class="card-img" alt="...">
            </a>
        </div>
    }
</div>